{
  "name": "JAVA Music ",
  "logo": "https://telegra.ph/file/9870433b0c155ecf2ad07.png",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/bhumiharsaurabh/katilmusicx",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "كود بايروجرام",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "توكن البوت اللي صنعته من @BotFather",
      "required": true
    },
    "BOT_USERNAME": {
      "description": "معرف بوتك بدون '@'",
      "required": true
    },
    "API_ID": {
      "description": "ايبي ايدي  تجيبه من هنا my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "ايبي هاش  تجيبه من هنا my.telegram.org/apps",
      "required": true
    },
    "OWNER_USERNAME": {
      "description": "معرفك بدون @",
      "required": true
    },
    "SUDO_USERS": {
      "description": "ايدي الادمنيه (مفصولة بمسافة).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "200"
    }
  }
}
